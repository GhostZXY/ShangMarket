<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sp.dao.StoresDao">
	<insert id="addStores" parameterType="stores">
		insert into stores (s_name,s_address,s_tel,u_id) 
		values (#{s_name},#{s_address},#{s_tel},#{u_id})
	</insert>
</mapper>